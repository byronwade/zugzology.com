{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "src/app/**/*.{ts,tsx}",
    "src/components/ui/**/*.tsx",
    "auth.ts",
    "proxy.ts"
  ],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": ["**/*.d.ts"],
  "ignoreDependencies": ["autoprefixer"],
  "next": {
    "entry": [
      "next.config.ts",
      "src/app/**/page.tsx",
      "src/app/**/layout.tsx",
      "src/app/**/loading.tsx",
      "src/app/**/error.tsx",
      "src/app/**/not-found.tsx",
      "src/app/**/route.ts",
      "auth.ts",
      "proxy.ts"
    ]
  }
}
